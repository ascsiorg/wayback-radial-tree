  "keywords": [
    "Wayback Machine",
    "Radial Tree Library"
  ],
  "dependencies": {
    "d3": "^5.16.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/core": "^7.20",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "^7.20",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.3.0",
    "babel-plugin-external-helpers": "6.22.0",
